<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportsScreen">
   <children>
      <Button fx:id="todayApptButton" layoutX="243.0" layoutY="119.0" mnemonicParsing="false" onAction="#clickTodayAppt" text="Todays Appointments" />
      <ComboBox fx:id="typeComboBox" layoutX="49.0" layoutY="188.0" prefWidth="150.0" promptText="Type" />
      <ComboBox fx:id="monthComboBox" layoutX="234.0" layoutY="188.0" prefWidth="150.0" promptText="Month" />
      <Button fx:id="typeMonthGenerateButton" layoutX="466.0" layoutY="188.0" mnemonicParsing="false" onAction="#generateTypeMonth" text="Generate" />
      <ComboBox fx:id="contactComboBox" layoutX="234.0" layoutY="263.0" prefWidth="150.0" promptText="By Contact" />
      <Button fx:id="generateContactButton" layoutX="466.0" layoutY="263.0" mnemonicParsing="false" onAction="#generateContact" text="Generate" />
      <Button fx:id="backButton" layoutX="30.0" layoutY="38.0" mnemonicParsing="false" onAction="#clickBackButton" text="Back" />
   </children>
</AnchorPane>
